{
  "name": "shadow-cljs.org",
  "version": "0.1.0",
  "description": "shadow-cljs site",
  "main": "index.js",
  "scripts": {
    "cdn": "rsync -avr --progress dist/* tiye.me:cdn/shadow-cljs-org",
    "tiye": "rsync -avr --progress dist/{index.html,manifest.json} tiye.me:repo/shadow-cljs/shadow-cljs.org/",
    "upload": "yarn cdn && yarn tiye",
    "release": "shadow-cljs release browser",
    "watch": "shadow-cljs watch browser",
    "compile-ssr": "shadow-cljs compile ssr",
    "html-dev": "env=dev node target/ssr.js && cp entry/manifest.json target/",
    "html": "node target/ssr.js && cp entry/manifest.json dist/",
    "index.html": "yarn compile-ssr && yarn html-dev",
    "del": "rm -rf dist/*",
    "serve": "http-server dist -s",
    "build": "yarn del && yarn release && yarn compile-ssr && yarn html"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:shadow-cljs/shadow-cljs.org.git"
  },
  "author": "jiyinyiyong",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^0.11.1",
    "shadow-cljs": "^2.0.131"
  }
}
